DATABASE_URL=postgresql://postgres:postgres@localhost:5432/moderation_db
SECRET_KEY=your-secret-key-change-in-production
ML_MODEL_CACHE_DIR=./model_cache
REDIS_URL=redis://localhost:6379/0
LOG_LEVEL=INFO
MODERATION_LATENCY_THRESHOLD_MS=100

# LLM Configuration (Optional)
# Choose one: openai, anthropic, ollama, or leave empty for fallback mode
LLM_PROVIDER=openai
LLM_API_KEY=your-api-key-here
LLM_MODEL=gpt-3.5-turbo

# OpenAI Options: gpt-3.5-turbo, gpt-4, gpt-4-turbo-preview
# Anthropic Options: claude-3-opus-20240229, claude-3-sonnet-20240229, claude-3-haiku-20240307
# Ollama Options: llama2, mistral, codellama (requires Ollama running locally)
